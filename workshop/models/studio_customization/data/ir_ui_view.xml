    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='x_name']" position="replace">
          <field display="full" name="x_studio_model"/>
        </xpath>
        <xpath expr="//kanban[1]/templates[1]" position="before">
          <field name="x_studio_customer_name"/>
          <field name="x_color"/>
        </xpath>
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]" position="attributes">
          <attribute name="color">x_color</attribute>
        </xpath>
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]/div[not(@name)][1]" position="before">
          <div class="o_dropdown_kanban dropdown" name="kanban_dropdown">
            <a class="dropdown-toggle o-no-caret btn" data-toggle="dropdown" href="#" aria-label="Dropdown menu" title="Dropdown menu" role="button">
              <span class="fa fa-bars fa-lg"/>
            </a>
            <div class="dropdown-menu" role="menu" name="studio_div_c6e0d9">
              <t t-if="widget.editable">
                <a data-type="set_cover" href="#" data-field="x_studio_cover_image_id" class="dropdown-item oe_kanban_action oe_kanban_action_a">
                    Set Cover Image
                </a>
                <a data-type="set_cover" href="#" data-field="x_studio_cover_image_id" class="dropdown-item oe_kanban_action oe_kanban_action_a">
                    Set Cover Image
                </a>
                <a type="edit" class="dropdown-item">Edit</a>
              </t>
              <t t-if="widget.deletable">
                <a type="delete" class="dropdown-item">Delete</a>
              </t>
              <ul class="oe_kanban_colorpicker" data-field="x_color"/>
            </div>
          </div>
          <field name="x_studio_field_rDZ6g"/>
        </xpath>
        <xpath expr="//kanban[1]/templates[1]/t[1]/div[1]/div[not(@name)][1]" position="after">
          <div class="oe_kanban_bottom_right" name="studio_div_6e83eb">
            <img t-att-src="kanban_image('res.partner', 'image_128', record.x_studio_customer_name.raw_value)" t-att-title="record.x_studio_customer_name.value" class="oe_kanban_avatar o_image_24_cover float-right"/>
          </div>
          <field t-if="record.x_studio_cover_image_id.value" name="x_studio_cover_image_id" widget="attachment_image"/>
          <field t-if="record.x_studio_cover_image_id.value" name="x_studio_cover_image_id" widget="attachment_image"/>
        </xpath>
      </data>
    </field>
    <field name="field_parent" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="inherit_id" ref="studio_customization.default_kanban_view__809444e4-2a59-4d84-b8b8-ff9219cc5d7a"/>
